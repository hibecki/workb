@model PPcore.Models.course_type

<form id="mem_train_recordForm" class="form-horizontal">
<div class="form-group">
    <label asp-for="ctype_code" class="col-md-3 control-label"></label>
    <div class="col-md-9">
        <input asp-for="ctype_code" class="form-control" />
        <span asp-validation-for="ctype_code" class="text-danger" />
    </div>
</div>
<div class="form-group">
    <label asp-for="ctype_desc" class="col-md-3 control-label"></label>
    <div class="col-md-9">
        <textarea asp-for="ctype_desc" class="form-control" rows="3" ></textarea>
        <span asp-validation-for="ctype_desc" class="text-danger" />
    </div>
</div>
<div class="form-group">
    <label asp-for="x_status" class="col-md-3 control-label"></label>
    <div class="col-md-9">
        @*@{
            var gradeList = new SelectList(new[]
            {
                new SelectListItem { Text = "Good", Value = "A", Selected = true },
                new SelectListItem { Text = "Pass", Value = "B"},
                new SelectListItem { Text = "Fail", Value = "F"},
            }, "Value", "Text");
        }*@
        <select asp-for="x_status" asp-items="ViewBag.x_status" class="form-control"></select>
    </div>
</div>


</form>
<div class="form-group">
    <div class="col-md-offset-3 col-md-9">
        <button id="course_typeSubmit" class="btn btn-success">บันทึก</button>
    </div>
</div>

